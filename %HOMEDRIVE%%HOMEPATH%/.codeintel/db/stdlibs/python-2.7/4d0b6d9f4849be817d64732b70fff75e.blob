      <scope doc="Transform list into a heap, in-place, in O(len(heap)) time." ilk="function" name="heapify" />
      <scope doc="Pop the smallest item off the heap, maintaining the heap&#10;invariant." ilk="function" name="heappop" />
      <scope doc="Push item onto heap, maintaining the heap invariant." ilk="function" name="heappush" />
      <scope doc="Push item on the heap, then pop and return the smallest item&#10;from the heap." ilk="function" name="heappushpop" />
      <scope doc="Pop and return the current smallest value, and add the new&#10;item." ilk="function" name="heapreplace" />
